<Application x:Class="snake_wpf.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:snake_wpf"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <SolidColorBrush x:Key="BgColor">#000</SolidColorBrush>
        <SolidColorBrush x:Key="GridBgColor">#312c40</SolidColorBrush>
        <SolidColorBrush x:Key="LineColor">#4F4867</SolidColorBrush>
        <SolidColorBrush x:Key="TextColor">#CCCC</SolidColorBrush>
        <SolidColorBrush x:Key="OverlayColor">#7F000000</SolidColorBrush>
        <FontFamily x:Key="MainFont">Assets/#Droid Sans Mono</FontFamily>
    </Application.Resources>
</Application>
